#include <iostream>

using namespace std;

int main()
{
    //Assignment Operator - = -- += -= *= /= %=
    //Arithmetic (Math) Operators - + = * / %
    
    //Data Type Conversion - Static_cast -- static_cast<DataType>(variable)
   
    int x = 7;
    int y = 2;
    
    cout << "Total: " << (static_cast<double>(x)/y) << endl;
    
    
    
            
    return 0;
}
